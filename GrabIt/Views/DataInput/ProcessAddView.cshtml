
<h4>Please enter the process details.</h4>
<script type="text/javascript">
    $(document).ready(function () {

        
 
       
        $('#TxtStartTime').timepicker();
       
    });
</script>


<div class="container" style="margin-top:15px;margin-left:15px;">
    <div class="row">
        @{Html.RenderAction("GetShiftType", "DataInput");}

        <div class="form-group">
            <label class="col-sx-2 control-label" for="TxtEndTime">Start Time:</label>
            <div class="col-sx-10">
               @* <input id="TxtStartTime" type="text" class="form-control" />*@
                <div class="input-append bootstrap-timepicker">
                    <input id="TxtStartTime" type="text" class="input-small">
                    <span class="add-on"><i class="icon-time"></i></span>
                </div>
            </div>
        </div>

        <div class="form-group">
            <label class="col-sx-2 control-label" for="TxtEndTime">End Time:</label>
            <div class="col-sx-10">
                <input id="TxtEndTime" type="text" class="form-control" />
            </div>
        </div>



        @{Html.RenderAction("GetProcessCat", "DataInput");}
        <div id="DivProcessType"></div>
        @*@{Html.RenderAction("GetProcessType", "DataInput");}<br />*@

        <input id="BtnAddProcess" type="button" class="btn btn-default" value="Add" />
        <div class="horizontalRule"></div>
    </div>
</div>